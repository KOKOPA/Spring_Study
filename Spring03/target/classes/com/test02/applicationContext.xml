<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="me" class="com.test02.BirthDto">
		<constructor-arg name="name" value="홍준성"></constructor-arg>
		<constructor-arg name="birth" value="2월 27일"></constructor-arg>
	</bean>
	
	<bean id="left" class="com.test02.BirthDto">
		<property name="name" value="임성현"></property>
		<property name="birth" value="2월 14일"></property>
	</bean>
	<bean id="right" class="com.test02.BirthDto">
		<property name="name" value="이태식"></property>
		<property name="birth" value="10월 4일"></property>
	</bean>
	
</beans>
